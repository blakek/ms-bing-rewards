{
  "name": "ms-bing-rewards",
  "version": "0.0.1",
  "description": "Automated runner for Bing / Microsoft Rewards",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --source-maps",
    "dev": "npm run start:dev",
    "lint": "standard --fix 'src/**/*.js' | snazzy",
    "start": "node .",
    "start:desktop": "node dist/index.js",
    "start:mobile": "node dist/mobile.js",
    "start:dev": "babel-node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blakek/ms-bing-rewards.git"
  },
  "author": "Blake Knight <oss.ideas@gmail.com> (http://blakek.me/)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/blakek/ms-bing-rewards/issues"
  },
  "homepage": "https://github.com/blakek/ms-bing-rewards#readme",
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "es2017"
    ]
  },
  "dependencies": {
    "axios": "^0.16.0",
    "nightmare": "^2.7.0",
    "snazzy": "^7.1.1",
    "standard": "^9.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0"
  }
}
